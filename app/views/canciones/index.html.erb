<p id="notice"><%= notice %></p>

<h1>Canciones</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @canciones.each do |cancione| %>
      <tr>
        <td><%= link_to 'Show', cancione %></td>
        <td><%= link_to 'Edit', edit_cancione_path(cancione) %></td>
        <td><%= link_to 'Destroy', cancione, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cancione', new_cancione_path %>
